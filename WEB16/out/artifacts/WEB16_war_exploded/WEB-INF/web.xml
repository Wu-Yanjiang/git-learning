<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>SendCookieServlet</servlet-name>
        <servlet-class>com.itheima.cookie.SendCookieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveCookieServlet</servlet-name>
        <servlet-class>com.itheima.cookie.RemoveCookieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCookieServlet</servlet-name>
        <servlet-class>com.itheima.cookie.GetCookieServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LastAccessTimeServlet</servlet-name>
        <servlet-class>com.itheima.lastaccesstime.LastAccessTimeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionServlet1</servlet-name>
        <servlet-class>com.itheima.session.SessionServlet1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionServlet2</servlet-name>
        <servlet-class>com.itheima.session.SessionServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SessionServlet2</servlet-name>
        <url-pattern>/session2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionServlet1</servlet-name>
        <url-pattern>/session1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LastAccessTimeServlet</servlet-name>
        <url-pattern>/lastAccess</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCookieServlet</servlet-name>
        <url-pattern>/getCookie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveCookieServlet</servlet-name>
        <url-pattern>/delCookie</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendCookieServlet</servlet-name>
        <url-pattern>/sendCookie</url-pattern>
    </servlet-mapping>
</web-app>